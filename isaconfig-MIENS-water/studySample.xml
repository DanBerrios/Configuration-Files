<isatab-config-file xmlns="http://www.ebi.ac.uk/bii/isatab_configuration#">
    <isatab-configuration table-name="studySample">
        <measurement term-label="[Sample]" term-accession="e.g. 12345"
            source-abbreviation="e.g. OBI"/>
        <technology term-label="" term-accession="e.g. 12345" source-abbreviation="e.g. OBI"/>
        <field header="Source Name" data-type="String" is-file-field="false"
            is-multiple-value="false" is-required="true">
            <description>Sources are considered as the starting biological material used in a
                study.</description>
            <default-value/>
            <generated-value-template>[INSTITUTION].Group-[GROUP_NO].Subject-[SUBJECT_NO]</generated-value-template>
        </field>
        <field header="Characteristics[biome]" data-type="Ontology term" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>select a environmental biome from ENVO environment (biome)</description>
            <default-value/>
            <recommended-ontologies>
                <ontology id="" abbreviation="ENVO" name="Environmental Ontology" version="Jun 2010">
                    <branch id="ENVO:00000428" name="biome"/>
                </ontology>
            </recommended-ontologies>
        </field>
        <field header="Characteristics[material]" data-type="Ontology term" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>select a enviromental material from ENVO environment
                (material)</description>
            <default-value/>
            <recommended-ontologies>
                <ontology id="" abbreviation="ENVO" name="Environmental Ontology" version="Jun 2010">
                    <branch id="ENVO:00010483" name="environmental matter"/>
                </ontology>
            </recommended-ontologies>
        </field>
        <field header="Characteristics[feature]" data-type="Ontology term" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>select a environmental feature from ENVO environment
                (feature)</description>
            <default-value/>
            <recommended-ontologies>
                <ontology id="" abbreviation="ENVO" name="Environmental Ontology" version="Jun 2010">
                    <branch id="ENVO:00002297" name="environmental feature"/>
                </ontology>
            </recommended-ontologies>
        </field>
        <field header="Characteristics[country]" data-type="String" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>The geographical origin of the sample as defined by the country. Country
                names should be chosen from the INSDC list
                (http://www.insdc.org/page.php?page=country), or the GAZ ontology
                (http://bioportal.bioontology.org/visualize/40651)</description>
            <default-value/>
        </field>
        <field header="Characteristics[collection date]" data-type="Date" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>The time of sampling, either as an instance (single point in time) or
                interval. In case no exact time is available, the date/time can be right
                truncated</description>
            <default-value>dd-MM-yyyy</default-value>
        </field>
        <field header="Characteristics[lat_lon]" data-type="String" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>The geographical origin of the sample as defined by latitude and longitude.
                The values should be reported in decimal degrees and in WGS84 system. Input should
                formatted as follows: +34.5,-23.5 instead of 34 30' 00 23 30' 00''</description>
            <default-value/>
            <value-format>[-+]\d+.\d+,[-+]\d+.\d+</value-format>
        </field>
        <field header="Characteristics[altitude_elevation]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>The geographical origin of the sample as defined by altitude and/or
                elevation. The altitude of the sample is the distance between surface and the
                sampled position. The elevation of the site is measured from the sea
                level</description>
            <default-value/>
        </field>
        <unit-field data-type="Ontology term" is-multiple-value="false" is-required="false">
            <description>elevation unit. Meter (m) is recommended."</description>
        </unit-field>
        <field header="Characteristics[depth]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>The geographical origin of the sample as defined by depth. Depth is defined
                as the distance from surface, e.g. for sediment samples depth is measured from
                sediment surface. Depth can be reported as an interval for subsurface
                samples</description>
            <default-value/>
        </field>
        <unit-field data-type="Ontology term" is-multiple-value="false" is-required="false">
            <description>depth unit. Meter (m) is recommended"</description>
        </unit-field>
        <structured-field name="Characteristics"/>
        <field header="Characteristics[alkalinity]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>alkalinity measurement</description>
            <default-value/>
        </field>
        <field header="Characteristics[alkyl_diethers]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of alkyl diethers</description>
            <default-value/>
        </field>
        <field header="Characteristics[aminopeptidase activity]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>measurement of aminopeptidase activity</description>
            <default-value/>
        </field>
        <field header="Characteristics[ammonium]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of ammonium</description>
            <default-value/>
        </field>
        <field header="Characteristics[atmospheric data]" data-type="String" is-file-field="false"
            is-multiple-value="true" is-required="false">
            <description>measurement of atmospheric data; can include multiple data</description>
            <default-value/>
        </field>
        <field header="Characteristics[bacterial carbon production]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>measurement of bacterial carbon production</description>
            <default-value/>
        </field>
        <field header="Characteristics[bishomohopanol]" data-type="Double" is-file-field="false"
            is-multiple-value="true" is-required="false">
            <description>concentration of bishomohopanol</description>
            <default-value/>
        </field>
        <field header="Characteristics[biomass]" data-type="String" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>amount of biomass; should include the name for the part of biomass
                measured, e.g. microbial, total. can include multiple measurements</description>
            <default-value/>
        </field>
        <field header="Characteristics[carbon/nitrogen ratio]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>ratio of amount or concentrations of carbon to nitrogen</description>
            <default-value/>
        </field>
        <field header="Characteristics[bromide]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of bromide</description>
            <default-value/>
        </field>
        <field header="Characteristics[chloride]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of chloride</description>
            <default-value/>
        </field>
        <field header="Characteristics[calcium]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of calcium</description>
            <default-value/>
        </field>
        <field header="Characteristics[current]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>measurement of current</description>
            <default-value/>
        </field>
        <field header="Characteristics[chemical administration]" data-type="String"
            is-file-field="false" is-multiple-value="true" is-required="false">
            <description>list of chemical compounds administered to the host or site where sampling
                occurred, and when (e.g. antibiotics, N fertilizer, air filter); can include
                multiple compounds. for CHEBI, see
                http://www.ebi.ac.uk/ontology-lookup/browse.do?ontName=CHEBI</description>
            <default-value/>
        </field>
        <field header="Characteristics[diether lipids]" data-type="Double" is-file-field="false"
            is-multiple-value="true" is-required="false">
            <description>concentration of diether lipids; can include multiple types of diether
                lipids</description>
            <default-value/>
        </field>
        <field header="Characteristics[chlorophyll]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of chlorophyll</description>
            <default-value/>
        </field>
        <field header="Characteristics[dissolved hydrogen]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of dissolved hydrogen</description>
            <default-value/>
        </field>
        <field header="Characteristics[density]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>density of sample</description>
            <default-value/>
        </field>
        <field header="Characteristics[dissolved inorganic phosphorus]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of dissolved inorganic phosphorus</description>
            <default-value/>
        </field>
        <field header="Characteristics[dissolved carbon dioxide]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of dissolved carbon dioxide</description>
            <default-value/>
        </field>
        <field header="Characteristics[dissolved oxygen]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of dissolved oxygen</description>
            <default-value/>
        </field>
        <field header="Characteristics[dissolved inorganic nitrogen]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of dissolved inorganic nitrogen</description>
            <default-value/>
        </field>
        <field header="Characteristics[glucosidase activity]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>measurement of glucosidase activity</description>
            <default-value/>
        </field>
        <field header="Characteristics[mean peak friction velocity]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>measurement of mean peak friction velocity</description>
            <default-value/>
        </field>
        <field header="Characteristics[dissolved organic carbon]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of dissolved organic carbon</description>
            <default-value/>
        </field>
        <field header="Characteristics[light intensity]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>measurement of light intensity</description>
            <default-value/>
        </field>
        <field header="Characteristics[mean friction velocity]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>measurement of mean friction velocity</description>
            <default-value/>
        </field>
        <field header="Characteristics[nitrite]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of nitrite</description>
            <default-value/>
        </field>
        <field header="Characteristics[miscellaneous parameter]" data-type="String"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>any other measurement performed or parameter collected, that is not listed
                here.should be given in the form of measurement name/parameter name=value and unit;
                can include multiple measurements or parameters</description>
            <default-value/>
        </field>
        <field header="Characteristics[magnesium]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of magnesium</description>
            <default-value/>
        </field>
        <field header="Characteristics[nitrate]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of nitrate</description>
            <default-value/>
        </field>
        <field header="Characteristics[organic nitrogen]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of organic nitrogen</description>
            <default-value/>
        </field>
        <field header="Characteristics[nitrogen]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of nitrogen (total)</description>
            <default-value/>
        </field>
        <field header="Characteristics[n-alkanes]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of n-alkanes; can include multiple n-alkanes</description>
            <default-value/>
        </field>
        <field header="Characteristics[organic matter]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of organic matter</description>
            <default-value/>
        </field>
        <field header="Characteristics[particulate organic carbon]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of particulate organic carbon</description>
            <default-value/>
        </field>
        <field header="Characteristics[organism count]" data-type="String" is-file-field="false"
            is-multiple-value="true" is-required="false">
            <description>organism count</description>
            <default-value/>
        </field>
        <field header="Characteristics[organic carbon]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of organic carbon</description>
            <default-value/>
        </field>
        <field header="Characteristics[phosphate]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of phosphate</description>
            <default-value/>
        </field>
        <field header="Characteristics[pH]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>pH measurement</description>
            <default-value/>
        </field>
        <field header="Characteristics[oxygenation status of sample]" data-type="List"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>oxygenation status of sample</description>
            <default-value/>
            <list-values>aerobe,anaerobe,facultative,microaerophilic,microanaerobe,obligate
                aerobe,obligate anaerobe</list-values>
        </field>
        <field header="Characteristics[particulate organic nitrogen]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of particulate organic nitrogen</description>
            <default-value/>
        </field>
        <field header="Characteristics[primary production]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>measurement of primary production</description>
            <default-value/>
        </field>
        <field header="Characteristics[perturbation]" data-type="String" is-file-field="false"
            is-multiple-value="true" is-required="false">
            <description>type of perturbation, e.g. chemical administration, physical disturbance,
                etc., coupled with time that perturbation occurred; can include multiple
                perturbation types</description>
            <default-value/>
        </field>
        <field header="Characteristics[petroleum hydrocarbon]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of petroleum hydrocarbon</description>
            <default-value/>
        </field>
        <field header="Characteristics[phaeopigments]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of phaeopigments; can include multiple
                phaeopigments</description>
            <default-value/>
        </field>
        <field header="Characteristics[sample storage location]" data-type="String"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>location at which sample was stored, usually name of a specific
                freezer/room</description>
            <default-value/>
        </field>
        <field header="Characteristics[phospholipid fatty acid]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of phospholipid fatty acid</description>
            <default-value/>
        </field>
        <field header="Characteristics[photon flux]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>measurement of photon flux</description>
            <default-value/>
        </field>
        <field header="Characteristics[pressure]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>pressure to which the sample is subject, in atmospheres</description>
            <default-value/>
        </field>
        <field header="Characteristics[sulfate]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of sulfate</description>
            <default-value/>
        </field>
        <field header="Characteristics[redox potential]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>redox potential, measured relative to a hydrogen cell, indicating oxidation
                or reduction potential</description>
            <default-value/>
        </field>
        <field header="Characteristics[salinity]" data-type="String" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>salinity measurement</description>
            <default-value/>
        </field>
        <field header="Characteristics[sample storage duration]" data-type="Date"
            is-file-field="false" is-multiple-value="true" is-required="false">
            <description>duration for which sample was stored</description>
            <default-value>dd-MM-yyyy</default-value>
        </field>
        <field header="Characteristics[suspended particulate matter]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of suspended particulate matter</description>
            <default-value/>
        </field>
        <field header="Characteristics[total nitrogen]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>total amount or concentration of nitrogenous substances</description>
            <default-value/>
        </field>
        <field header="Characteristics[total particulate carbon]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>total particulate carbon content</description>
            <default-value/>
        </field>
        <field header="Characteristics[total phosphorus]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>total phosphorus content</description>
            <default-value/>
        </field>
        <field header="Characteristics[sulfite]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of sulfide</description>
            <default-value/>
        </field>
        <field header="Characteristics[soluble reactive phosphorus]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>concentration of soluble reactive phosphorus</description>
            <default-value/>
        </field>
        <field header="Characteristics[silicate]" data-type="String" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>concentration of silicate</description>
            <default-value/>
        </field>
        <field header="Characteristics[sample storage temperature]" data-type="String"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>temperature at which sample was stored, e.g. -80</description>
            <default-value/>
        </field>
        <field header="Characteristics[temperature]" data-type="Double" is-file-field="false"
            is-multiple-value="false" is-required="false">
            <description>temperature of the sample at time of sampling</description>
            <default-value/>
        </field>
        <field header="Characteristics[total inorganic nitrogen]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>total inorganic nitrogen content</description>
            <default-value/>
        </field>
        <field header="Characteristics[total depth of water column]" data-type="Double"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>measurement of total depth of water column</description>
            <default-value/>
        </field>
        <protocol-field protocol-type="environmental material collection"/>
        <field header="Parameter Value[sample collection device]" data-type="String"
            is-file-field="false" is-multiple-value="false" is-required="false">
            <description>indicate the instrument used to harvest and collect the
                sample</description>
            <default-value>PET bottle</default-value>
        </field>
        <field header="Sample Name" data-type="String" is-file-field="false"
            is-multiple-value="false" is-required="true">
            <description>Samples represent major outputs resulting from a protocol application other
                than the special case outputs of Extract or a Labeled Extract.</description>
            <default-value/>
            <generated-value-template>[INSTITUTION].Group-[GROUP_NO].Subject-[SUBJECT_NO].[SAMPLE_EXTRACT]</generated-value-template>
        </field>
        <structured-field name="Factors"/>
    </isatab-configuration>
</isatab-config-file>
